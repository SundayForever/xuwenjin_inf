package cn.com.broadtext.rest.entity;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

import com.fasterxml.jackson.annotation.JsonIdentityInfo;
import com.fasterxml.jackson.annotation.ObjectIdGenerators;
import com.github.jasminb.jsonapi.annotations.Links;
import com.github.jasminb.jsonapi.annotations.Type;

@Entity
@Table(name = "SYS_COLUMN")
@Type("column")
@JsonIdentityInfo(generator = ObjectIdGenerators.StringIdGenerator.class, property = "pkId")
public class Column {
	
	public static class ColumeMeta {
		public String token;

		public String getToken() {
			return token;
		}
	}
	
	@Id
    @GeneratedValue(strategy = GenerationType.AUTO)
	@com.github.jasminb.jsonapi.annotations.Id
	private String pkId;
	
	private String name;
	
	private String description;
	
	private String isNotGround;
	
	private String icon;
	
	private long sort;
	
//	@Relationship(value = "video", resolve = true)
//	private List<Video> video;
	
	@Links
	private com.github.jasminb.jsonapi.Links links;
	
	public String getPkId() {
		return pkId;
	}

	public void setPkId(String pkId) {
		this.pkId = pkId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getIsNotGround() {
		return isNotGround;
	}

	public void setIsNotGround(String isNotGround) {
		this.isNotGround = isNotGround;
	}

	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}

	public long getSort() {
		return sort;
	}

	public void setSort(long sort) {
		this.sort = sort;
	}

//	public List<Video> getVideo() {
//		return video;
//	}
//
//	public void setVideo(List<Video> video) {
//		this.video = video;
//	}
}
